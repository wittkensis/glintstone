  BABYLEM -.->|"competing\nlemmatizations"| lemmat
  BABYLEM -.->|"model run"| ann_runs
  ML_OCR -.->|"automated\ndetection"| sign_ann
  ML_OCR -.->|"model run"| ann_runs

